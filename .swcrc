{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "dynamicImport": true,
        "decorators": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es5",
      "externalHelpers": false,
      "keepClassNames": true,
      "loose": false,
      "minify": {
        "compress": false,
        "mangle": false
      },
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"],
        "@config": ["config"],
        "@modules/*": ["modules/*"],
        "@middlewares/*": ["middlewares/*"],
        "@routes/*": ["routes/*"],
        "@utils/*": ["utils/*"]
      }
    },
    "module": {
      "type": "commonjs"
    }
  }
